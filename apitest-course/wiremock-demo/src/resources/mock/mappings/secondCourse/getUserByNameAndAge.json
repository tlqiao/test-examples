{
  "request": {
    "method": "GET",
    "urlPathPattern": "/api/getUserByNameAndAge",
    "queryParameters": {
      "name": {
        "matches": "[a-z]*"
      },
      "age": {
        "matches": "[0-9]*"
      }
    }
  },
  "response": {
    "status": 200,
    "body": "get user by name and age successfully"
  }
}